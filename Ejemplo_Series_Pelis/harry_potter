<script type="text/javascript">
	
	function save_cookies_configuration(mode)
	{
		cookies_ads = ($('.cookies_ads').is(':checked')) ? 1 : 0;
		cookies_sta = ($('.cookies_sta').is(':checked')) ? 1 : 0;

		setCookie('pm_cookies_ads', cookies_ads, false, '/', '.'+current_domain, null, (31600000 * 1000));
		setCookie('pm_cookies_sta', cookies_sta, false, '/', '.'+current_domain, null, (31600000 * 1000));
		setCookie('pm_cookies_sho', 1, false, '/', '.'+current_domain, null, (31600000 * 1000));

		if(mode)
		{	
			popup_m_n('use_popup', 'cookies_configuration');
			popup_m_n('ocultar', '');
			popup_m_n('use_popup', 'main');
		}
	}

	cookies_ads = +'1';
	cookies_sta = +'1';

	function cookies_configuration()
	{

		html = '<div style="padding: 20px;">Utilizamos cookies propias y de terceros para mejorar la experiencia de navegación, desde aquí puede configurar qué cookies se utilizaran. Hay algunas cookies que no se pueden desactivar, ya que son esenciales para el funcionamiento de PlayMax (Cookies de configuración, sesion, anuncios no personalizados, etc)<br><br>Puede obtener más información sobre las cookies en <a href="./cookie.php">Política de Cookies</a> y <a href="./terms_and_conditions.php">Términos y Condiciones</a>.<br><div style="height: 20px; line-height: 20px; margin-top: 16px;"><div class="checkbox s20 '+(!cookies_ads ? 'n' : '')+'p" style="float: left;"><input type="checkbox" name="cookies_ads" class="cookies_ads"'+(cookies_ads ? ' checked' : '')+'></div>Cookies para los anuncios personalizados (Google AdSense).</div><div style="height: 20px; line-height: 20px; margin-top: 16px;"><div class="checkbox s20 '+(!cookies_sta ? 'n' : '')+'p" style="float: left;"><input type="checkbox" name="cookies_sta" class="cookies_sta"'+(cookies_sta ? ' checked' : '')+'></div>Cookies para las estadísticas (Google Analytics).</div><br><div class="c_button" onclick="save_cookies_configuration(1)">Guardar</div></div>';

		width = 500;
		height = 277;

		if(width > rd_width() - 20)
			width = rd_width() - 20;

		if(height > rd_height() - 120)
			height = rd_height() - 120;

		popup_m_n('use_popup', 'cookies_configuration');
		popup_m_n('restart', '');
		popup_m_n('width', width);
		popup_m_n('height', height);
		popup_m_n('title', 'Configuración de las Cookies');
		popup_m_n('content', html);
		popup_m_n('function_c', 'save_cookies_configuration(0);');
		popup_m_n('mostrar', '');
		popup_m_n('use_popup', 'main');

		form_events();
	}

	function save_cookies_configuration_all(mode)
	{
		cookies_ads = 1;
		cookies_sta = 1;

		setCookie('pm_cookies_ads', cookies_ads, false, '/', '.'+current_domain, null, (31600000 * 1000));
		setCookie('pm_cookies_sta', cookies_sta, false, '/', '.'+current_domain, null, (31600000 * 1000));
		setCookie('pm_cookies_sho', 1, false, '/', '.'+current_domain, null, (31600000 * 1000));
	}

	function cookies_configuration_little()
	{

		html = '<div style="padding: 20px;">Utilizamos cookies propias y de terceros para mejorar la experiencia de navegación, desde aquí puede configurar qué cookies se utilizaran. Hay algunas cookies que no se pueden desactivar, ya que son esenciales para el funcionamiento de PlayMax (Cookies de configuración, sesion, anuncios no personalizados, etc)<br><br>Puede obtener más información sobre las cookies en <a href="./cookie.php">Política de Cookies</a> y <a href="./terms_and_conditions.php" onclick="cookies_configuration()">Términos y Condiciones</a>.<br><br><div class="c_button" onclick="cookies_configuration()">Configuración de las Cookies</div></div>';

		width = 500;
		height = 205;

		if(width > rd_width() - 20)
			width = rd_width() - 20;

		if(height > rd_height() - 120)
			height = rd_height() - 120;

		popup_m_n('use_popup', 'cookies_configuration');
		popup_m_n('restart', '');
		popup_m_n('width', width);
		popup_m_n('height', height);
		popup_m_n('title', 'PlayMax utiliza Cookies propias y de terceros');
		popup_m_n('content', html);
		popup_m_n('function_c', 'save_cookies_configuration_all();');
		popup_m_n('mostrar', '');
		popup_m_n('use_popup', 'main');

		form_events();
	}

	
	$(document).ready(function(){

		cookies_configuration_little();

	});
	

</script>
